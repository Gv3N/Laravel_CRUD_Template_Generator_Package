{
    "name": "gv3n/laravel_crud_template_generator",
    "description": "A Laravel package to generate CRUD operations based on templates",
    "type": "library",
    "keywords": ["laravel", "crud", "generator"],
    "autoload": {
        "psr-4": {
            "Gv3N\\LaravelCrudTemplateGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gv3N",
            "email": "baktajivan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gv3n\\LaravelCrudTemplateGenerator\\Providers\\CrudServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
